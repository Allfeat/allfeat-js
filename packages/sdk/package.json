{
  "name": "@allfeat/sdk",
  "version": "0.1.0",
  "description": "High-level package which re-export others useful SDK packages.",
  "author": "Allfeat labs. <hello@allfeat.com>",
  "type": "module",
  "exports": {
    ".": {
      "default": "./dist/index.js",
      "types": "./dist/index.d.ts"
    }
  },
  "scripts": {
    "build": "tsc",
    "build:clean": "rm -rf ./dist",
    "lint": "eslint . --max-warnings 0",
    "typecheck": "tsc --noEmit"
  },
  "keywords": [],
  "license": "MIT",
  "dependencies": {
    "@allfeat/chaintypes": "workspace:*",
    "@allfeat/client": "workspace:*",
    "@allfeat/eslint-config": "workspace:*",
    "@allfeat/midds": "workspace:*",
    "@allfeat/typescript-config": "workspace:*"
  },
  "packageManager": "pnpm@10.10.0",
  "publishConfig": {
    "access": "public"
  },
  "devDependencies": {
    "eslint": "^9.27.0",
    "typescript": "^5.8.3"
  }
}
